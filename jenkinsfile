pipeline {
  agent any

  stages {
    stage('Inicial') {
      steps {
        echo 'Jamel...'
      }
    }
    stage('PM2 ADD') {
      steps {
        script {
          sh 'cd /home/ubuntu/repos/web-server-teste && npm i && npm start'
        }
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }
  }
}