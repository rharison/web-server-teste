pipeline {
  agent any

  stages {
    stage('Inicial') {
      steps {
        echo 'Jamel...'
      }
    }
    stage('PM2 ADD') {
      steps {
        script {
          sh 'pm2 start index.js'
        }
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }
  }
}