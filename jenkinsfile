pipeline {
  agent any

  stages {
    stage('PWD') {
      steps {
        script {
          sh 'pwd'
        }
      }
    }

    stage('PM2 START') {
      steps {
        script {
          sh 'cd /home/ubuntu/repos/web-server-teste && pm2 start index.js'
        }
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }
  }
}